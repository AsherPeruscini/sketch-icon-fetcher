{
  "name": "Logo fetcher",
  "description": "Plugin to place logos from various sources",
  "author": "Tobi Kremer",
  "authorEmail": "tobias.kremer@gmail.com",
  "homepage": "http://github.com/soulchild/sketch-logo-fetcher",
  "identifier": "de.funkreich.sketch.logo-fetcher",
  "version": "0.5",
  "compatibleVersion": "3.8.3",
  "commands": [
    {
      "script": "src/clearbit.cocoascript",
      "handler": "placeClearbitLogo",
      "shortcut": "cmd ctrl l",
      "name": "Insert company logo from Clearbit (low-res PNG)",
      "identifier": "clearbit"
    },
    {
      "script": "src/svg.cocoascript",
      "handler": "placeSVGLogo",
      "shortcut": "cmd ctrl k",
      "name": "Insert user-contributed brand logo (SVG)",
      "identifier": "svg"
    }
  ],
  "menu": {
    "items": [
      "svg",
      "clearbit"
    ],
    "title": "Logo fetcher"
  }
}
